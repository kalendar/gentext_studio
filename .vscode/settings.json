{
  "python.analysis.autoImportCompletions": true,
  "python.analysis.typeCheckingMode": "strict",
  "emmet.includeLanguages": { "jinja-html": "html" },
  "cSpell.words": [
    "dotenv",
    "enctype",
    "fastapi",
    "GOLE",
    "GOLEs",
    "Groq",
    "jinjax",
    "leaflock",
    "lucide",
    "OELMs",
    "pydantic",
    "redoc",
    "TREEBEARD",
    "uvicorn",
    "venv"
  ],
  "[jinja-html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.jinja": "jinja-html"
  },
  "tailwindCSS.includeLanguages": {
    "jinja-html": "html"
  }
}
